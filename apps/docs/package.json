    "test:examples": "bun test __tests__/doctest.test.ts"
  },
  "devDependencies": {
    "documentation": "^14.0.3",
    "mintlify": "^4.2.331",
    "remark-mdx": "^3.1.1",
    "remark-parse": "^11.0.0",
    "unified": "catalog:",
    "unist-util-visit": "^5.1.0"
  }
}
